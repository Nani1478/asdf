			</munit:set-event>
		</munit:behavior>
		<munit:execution >
			<flow-ref doc:name="Flow-ref to munit-demo-test2Flow" doc:id="fd86bd47-0ccc-401e-8e12-a23aae98a562" name="munit-demo-test2Flow"/>
